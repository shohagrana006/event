
{% if app.request.locale == "ar" %}
    {% set locale = "en" %}
{% else %}
    {% set locale = app.request.locale %}
{% endif %}

<!DOCTYPE html>
<html lang="{% if app.request.locale == "ar" %}en{% else %}{{ app.request.locale }}{% endif %}">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>offline ticket</title>
    <style>
        /* Add your styles here */
        body {
            margin: 0 auto;
            padding: 0;
            font-family: Arial, sans-serif;
            background-color: #e4e4e4;
            color: black;
            text-align: center;
        }

        .container {
            width: 100%;
            max-width: 475px;
            margin: auto;
            /* padding: 22px; */
            background-color: #fff;
        }

        .logo {
            background-color: #3e7cf7;
            text-align: center;
            font-size: 34px;
            padding-top: 40px;
            font-weight: 600;
            padding-bottom: 40px;
            display: block;
            width: 100%;
            line-height: 20px;
            color: #fff;
        }

        .logo img {
            width: 30px;
            height: 30px;
            object-fit: cover;
            margin-top: 6px;
            opacity: 1 !important;
        }

        .ticket-details {
            margin-top:33px;
            font-family: 'Nunito Sans', sans-serif;
            font-weight: 600;
            font-size: 28px;
            padding-bottom: 10px;
        
        }

        .organizer-name-title {
            /* font-family: 'Fraunces', serif; */
            font-family: Arial, Helvetica, sans-serif;
            /* font-weight: 600; */
            font-size: 20px;
            /* line-height: 35px; */
            padding-bottom: 40px;
        }

        .organizer-name {
            padding: 0px 75px;
            padding-bottom: 0px;
            border-bottom: 1px solid rgba(174, 168, 168, 0.709);
            padding-bottom: 5px;
            width: 40%;
            margin-left: auto;
            margin-right: auto;
            font-weight: 700;
            font-size: 22px;
        }

        .event-online {
            padding-top: 5px;
            font-weight: 700;
            font-size: 20px;
        }

        .ticket-image {
            background: transparent url('data:image/png;base64,') no-repeat;
            background-size: cover;
            background-position: center;
            opacity: 1;
            width: 100%;
            height: 493px;
            position: absolute;
            top: 20%;
        }

        .ticket-date {
            background-color: #3e7cf7;
            border-radius: 25px;
            cursor: pointer;
            box-shadow: #3e7cf7 0px 4px 14px;
            font-size: 20px;
            padding: 15px 20px;
            margin-top: 20px;
            margin-bottom: 30px;
            width: 170px;
            margin-left: auto;
            margin-right: auto;
            color: #ffffff;
        }

        .order-number p{
            font-style: italic;
            color: #808080;
            font-size: 14px;
            font-weight: 300;
            letter-spacing: 1px;
            margin-bottom: 20px;
        }

        .ticket-info-container{
            margin-bottom: 30px;
        }

        .ticket-info-container::after {
            content: "";
            clear: both;
            display: table;
        }

        .ticket-info {
            float: left;
            width: 150px;
            height: 70px;
        }

        .ticket-info p {
            font-style: italic;
            color: #808080;
            font-size: 12px;
            padding-bottom: 10px;
        }

        .ticket-type {
            font-size: 10px;
            font-weight: 800;
        }

        .link-online {
            font-size: 23px;
        }

        .online-link {
            font-size: 13px;
            color: #d2cfd7;
            margin-top: 20px;
        }

        .join-button {
            background-color: #3e7cf7;
            color: white;
            text-decoration: none;
            font-size: 13px;
            padding: 10px;
            border-radius:17px;
            display: block;
            margin-top: 20px;
            font-weight: 700;
            width: 50px;
            margin-left: auto;
            margin-right: auto;
            margin-top: 20px;
            outline: none;

        }

        a.join-button:hover{
            background-color: none !important;
        }

        .ticket-number {
            font-size: 12px;
            padding-top: 80px;
            font-weight: 700;
        }
        .ticket-footer{
            margin-top: 90px;
        }
        .ticket-footer small{
            font-size: 10px;
            
        }
        .qr-barcode-wrapper{
            background-color: white;
            margin: 0 auto;
            margin-top: 30px;
            padding: 10px;
            width: 125px;
            margin-bottom: 8px;
            
        }
    </style>
</head>

<body>

    {% set i = 0 %}
    {% for orderElement in order.orderelements if (eventDateTicketReference == "all" or eventDateTicketReference == orderElement.eventticket.reference)  %}
    {% for ticket in orderElement.tickets %}
    <div class="container">
        <div class="logo">
            <img src="data:image/png;base64," alt="">
                {{ orderElement.eventticket.eventdate.event.translate(locale).name }}
                
            <img src="data:image/png;base64," alt="">
        </div>
        <div class="order-number">
            <p>{{ "ORDER"|trans({}, "messages", locale) }} #{{ order.reference }}</p>
        </div>
        <div class="ticket-details">
            ORGANIZER
        </div>
        <div class="organizer-name-title">
           {{ orderElement.eventticket.eventdate.event.organizer.name }}
        </div>
      
        <div class="organizer-name">
            BEGINS {{ orderElement.eventticket.eventdate.startdate|date('g:i A') }}
        </div>
        <div class="event-online">  
            {% if orderElement.eventticket.eventdate.venue %}
                ON-SITE
            {% else %}
                ONLINE
            {% endif %}
        </div>
        <div class="ticket-image"></div>
        <div class="ticket-date">
            {{ orderElement.eventticket.eventdate.startdate|date('F j / Y') }}
        </div>
        
        <div class="ticket-info-container">
            <div style="border-bottom: 1px solid rgba(174, 168, 168, 0.709); margin-bottom: 35px;"></div>
            <div>
                {# <h4> 

                     {% if name_on_tickets is defined and name_on_tickets is not null %}
                        {% if i < name_on_tickets|length %}
                            {{ name_on_tickets[i] }}
                            {% set i = i + 1 %}
                        {% else %}
                            {{ order.user.getCrossRoleName }}
                            {% if order.user.hasRole("ROLE_POINTOFSALE") and order.payment.firstname and order.payment.lastname %}
                                ({{ order.payment.firstname ~ " " ~ order.payment.lastname }})
                            {% endif %}
                        {% endif %}
                    {% else %}
                        {{ order.user.getCrossRoleName }}
                        {% if order.user.hasRole("ROLE_POINTOFSALE") and order.payment.firstname and order.payment.lastname %}
                            ({{ order.payment.firstname ~ " " ~ order.payment.lastname }})
                        {% endif %}
                    {% endif %}

                </h4> #}

                <h4>
                     {% if user_ticket_name is defined and user_ticket_name is not null %}
                        {% if user_ticket_name[i] is defined and user_ticket_name[i][0] is defined and user_ticket_name[i][0] == orderElement.eventticket.eventdate.event.id %}
                            {{ user_ticket_name[i][1] }}
                            {% set i = i + 1 %}
                        {% else %}
                            {{ order.user.getCrossRoleName }}
                            {% if order.user.hasRole("ROLE_POINTOFSALE") and order.payment.firstname and order.payment.lastname %}
                                ({{ order.payment.firstname ~ " " ~ order.payment.lastname }})
                            {% endif %}
                        {% endif %}
                    {% else %}
                        {{ order.user.getCrossRoleName }}
                        {% if order.user.hasRole("ROLE_POINTOFSALE") and order.payment.firstname and order.payment.lastname %}
                            ({{ order.payment.firstname ~ " " ~ order.payment.lastname }})
                        {% endif %}
                    {% endif %}
                </h4>
            </div>
            <div class="ticket-info" style="border-right: 1px solid white;">
                <p>TICKET TYPE</p>
                <div class="ticket-type">{{ orderElement.eventticket.name }}</div>
            </div>
            <div class="ticket-info" style="border-right: 1px solid white;">
                <p>ORDER DATE</p>
                <div class="ticket-type order-date">
                     {{  order.createdAt|date('F j / Y') }}
                </div>
            </div>
            <div class="ticket-info">
                <p>TICKET PRICE</p>
                <div class="ticket-type ticket-price">
                    {{ orderElement.eventticket.free ? "FREE"|trans({}, "messages", locale) : ((services.getSetting('currency_position') == 'left' ? services.getSetting('currency_symbol') : '') ~ orderElement.displayUnitPrice() ~ (services.getSetting('currency_position') == 'right' ? services.getSetting('currency_symbol') : '')) }}
                </div>
            </div>
        </div>
       
        {% if orderElement.eventticket.eventdate.venue %}
            
            <div class="vanue-location">
                <h3>ADDRESS</h3>
                {{ orderElement.eventticket.eventdate.venue.stringifyAddress }}
            </div>
            <div class="vanue-qr-code">
                <div class="qr-barcode-wrapper">
                        {{ barcode({code: ticket.reference, type: 'qrcode', format: 'html'}) }}
                    </div>
                    <small>SCAN HERE</small>
            </div>

            <style>
            .vanue-qr-code small{
                font-size: 10px;
            }

            .vanue-location h3{
                padding-bottom: 5px;
            }

            
            .ticket-number{
                padding-top: 20px !important;
            }
            .ticket-footer{
                margin-top: 15px !important;
            }

            .ticket-info-container{
                margin-bottom: 20px !important;
            }
            </style>
        {% else %}
            {% if link is not empty %}
            {# <div class="link-online">
                LINK ONLINE
            </div>
            <div class="online-link">
                {% if link is not empty %}
                {{ link|trans({}, "messages", locale) }}
                {% endif %}
            </div> #}
               <a href="{{ link|trans({}, "messages", locale) }}" class="join-button" rel="noopener" target="_blank">JOIN</a>
            {% endif %}
        {% endif %}
        
        <div class="ticket-number">
            {{ "Ticket"|trans({}, "messages", locale) }} #{{ ticket.reference }}
        </div>

        <div class="ticket-footer">
            <small>https://www.keos.co/Keos - Event Management And Ticket Sales</small>
        </div>
    </div>
    {% endfor %}
    {% endfor %}
</body>

</html>
