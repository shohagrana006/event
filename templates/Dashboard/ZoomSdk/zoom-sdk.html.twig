    <script>
        var socket = io("{{ nodeServer }}", {
            secure: true,
        })
        socket.on("start_quiz_{{ event_meeting['meeting_id'] }}", function(response) {
            $('#quiz_start_modal').modal('show');
        });

        socket.on("close_quiz_{{ event_meeting['meeting_id'] }}", function(response) {
            $('#quiz_iframe').html(`
                <iframe src="{{ quizApiUrl }}" height="200" width="300"  title="quiz"></iframe>
            `)
            $('#quiz_start_modal').modal('hide');
        });
    </script>

