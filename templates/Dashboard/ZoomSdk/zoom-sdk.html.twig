    <script>
        ZoomMtg.preLoadWasm()
        ZoomMtg.prepareWebSDK()
        
        var authEndpoint = '{{ zoomAuthEndPoint }}'
        var sdkKey = '{{ sdkKey }}'
        var zoom_client_secret = '{{ sdk_secret }}'
        var meetingNumber = '{{ event_meeting['meeting_id'] }}'
        var passWord = '{{ event_meeting['password'] }}'
        var role = 0
        var userName = 'Keos'
        var userEmail = ''
        var registrantToken = ''
        var zakToken = ''
        var leaveUrl = '{{ leaveUrl }}'

        function getSignature() {
            fetch(authEndpoint, {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                    meetingNumber: meetingNumber,
                    sdkKey: sdkKey,
                    zoom_client_secret: zoom_client_secret,
                    role: role
                })
            }).then((response) => {
                return response.json()
            }).then((data) => {
                // console.log(data)
                startMeeting(data.signature)

                if ($("#zmmtg-root").attr('style') !== undefined) {
                    $(".section-content").hide();
                }
            }).catch((error) => {
                // alert('SDK key Not Valid')
                console.log(error)
            })
        }

        function startMeeting(signature) {

            document.getElementById('zmmtg-root').style.display = 'block';
            window.tracker.track('Meeting In', {
                Type: 'Click',
                Action: 'Cliente',
                passWord: passWord,
                userName: userName,
                userEmail: userEmail,
            }).then((resp) => {
                ZoomMtg.init({
                    leaveUrl: leaveUrl,
                    patchJsMedia: true,
                    success: (success) => {
                        console.log(success);
                        ZoomMtg.join({
                            signature: signature,
                            sdkKey: sdkKey,
                            meetingNumber: meetingNumber,
                            // zoom_client_secret:zoom_client_secret,
                            passWord: passWord,
                            userName: userName,
                            userEmail: userEmail,
                            // tk: registrantToken,
                            // zak: zakToken,
                            success: (success) => {
                                console.log(success);
                            },
                            error: (error) => {
                                console.log(error);
                            },
                        });
                    },
                    error: (error) => {
                        console.log(error);
                    }
                });
            }).catch((err) => {
                console.error(err);
            });

        }
    </script>

